operator {
	==|<=|>=|!=|<|>
}

access_id {
	public|private|protected
}

name {
	[\w_]+
}

generic {
	<<name>>
}

testRule {
    // The spaces in here will be lost...which is ok I guess
    (this is the \{test\}){2,5}
}


result_identifier {
	select|where
}

lambda_filter -> LambdaTranslator {
	(<name>)(<generic>)\s*(<name>)\s*=\s*
    from\s+(<name>)\s+(<result_identifier>)\s+(\w)+\s*=>\s*(.*);
}

